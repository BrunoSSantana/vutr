{
  "name": "vutr",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --minify",
    "start": "node dist/server.js",
    "start:prod": "npm run build && npm run start",
    "prisma:generate": "prisma generate --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migrate:push": "prisma migrate deploy --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/infra/repositories/prisma/schema.prisma",
    "deploy:db": "npm run prisma:generate && npm run prisma:migrate:push",
    "test": "vitest run --dir src/domains/**/usecases --no-threads && vitest run --dir src/domains/**/validation --no-threads",
    "test:watch": "vitest --dir src/usecases",
    "test:e2e": "vitest run --dir src/domains/**/http --no-threads && vitest run --dir src/domains/**/repositories --no-threads",
    "test:e2e:watch": "vitest watch --dir src/domains/**/infra/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "4.7.1",
    "@as-integrations/fastify": "2.0.0",
    "@prisma/client": "4.14.1",
    "buffer-from": "^1.1.2",
    "dotenv": "16.0.3",
    "esbuild": "^0.17.18",
    "fastify": "4.17.0",
    "firebase-admin": "11.8.0",
    "get-tsconfig": "^4.5.0",
    "graphql": "16.6.0",
    "graphql-yoga": "3.9.1",
    "source-map": "^0.6.1",
    "source-map-support": "^0.5.21",
    "vite-tsconfig-paths": "4.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.1.0",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-c8": "0.31.0",
    "prisma": "4.14.1",
    "supertest": "6.3.3",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "vitest": "0.31.0"
  },
  "description": ""
}
